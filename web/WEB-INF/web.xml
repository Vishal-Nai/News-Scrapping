    <servlet-mapping>
        <servlet-name>world</servlet-name>
        <url-pattern>/world</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Buisness</servlet-name>
        <url-pattern>/Buisness</url-pattern>
    </servlet-mapping>
