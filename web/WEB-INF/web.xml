    <servlet>
        <servlet-name>index_1</servlet-name>
        <servlet-class>index_1</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SecondServlet</servlet-name>
        <servlet-class>SecondServlet</servlet-class>
