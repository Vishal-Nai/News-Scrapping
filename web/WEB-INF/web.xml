        <servlet-name>BreakingNews</servlet-name>
        <url-pattern>/BreakingNews</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Sports</servlet-name>
        <url-pattern>/Sports</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Entertainment</servlet-name>
        <url-pattern>/Entertainment</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>index_1</servlet-name>
        <url-pattern>/index_1</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SecondServlet</servlet-name>
        <url-pattern>/SecondServlet</url-pattern>
    </servlet-mapping>
