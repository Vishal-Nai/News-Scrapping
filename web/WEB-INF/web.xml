    <servlet-mapping>
        <servlet-name>Politics</servlet-name>
        <url-pattern>/Politics</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>india</servlet-name>
        <url-pattern>/india</url-pattern>
